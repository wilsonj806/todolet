{
  "name": "todolet",
  "version": "0.8.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wilsonj806/todolet.git"
  },
  "scripts": {
    "heroku-prebuild": "npm i --only=dev && echo Dependencies installed",
    "build:client": "webpack --config config/webpack.config.js --env.NODE_ENV=production && echo Webpack client build successful",
    "build:server": "webpack --config config/webpack.server.config.js --env.NODE_ENV=production && echo Webpack server build successful",
    "heroku-postbuild": "npm run build:server && npm run build:client && npm run gulp && npm prune && echo Packages pruned",
    "gulp": "gulp",
    "start": "node dist/server.bundle.js",
    "dev:client": "webpack --config config/webpack.config.js --env.NODE_ENV=development",
    "dev:server": "nodemon -e ts,tsx --exec babel-node --extensions '.ts','.tsx','.js' server.ts",
    "watch-ts": "tsc --watch",
    "test:ts": "jest --config=config/jest.config.js --watch",
    "test:tsx": "jest --config=config/react.jest.config.js --watch",
    "coverage": "jest --coverage --config=config/jest.config.js && jest --coverage --config=config/react.jest.config.js && mkdir coverage && touch coverage/lcov.info && lcov-result-merger 'todolet_coverage_*/lcov.info' './coverage/lcov.info' && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./todolet_coverage_* && echo 'Coverage reporting done'",
    "analyze": "source-map-explorer 'dist/*.bundle.js'"
  },
  "dependencies": {
    "@material-ui/core": "4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@types/sequelize": "^4.28.9",
    "@use-hooks/axios": "^1.3.0",
    "axios": "^0.21.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "connect-flash": "0.1.1",
    "connect-pg-simple": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "8.0.0",
    "express": "4.17.3",
    "express-messages": "1.0.1",
    "express-session": "1.16.1",
    "express-validator": "5.3.1",
    "notistack": "^0.9.7",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.1",
    "redux-thunk": "^2.3.0",
    "sequelize": "^6.3.5",
    "window-or-global": "1.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@jedmao/redux-mock-store": "^2.0.3",
    "@shelf/jest-mongodb": "^1.1.3",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-pg-simple": "^4.2.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.15",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.4",
    "@types/passport": "^1.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.22",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-loader": "8.0.5",
    "concurrently": "^3.6.1",
    "coveralls": "^3.0.7",
    "del": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-watch": "^5.1.2",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "husky": "^4.2.5",
    "jest": "24.7.1",
    "lcov-result-merger": "^3.1.0",
    "lint-staged": "^10.2.9",
    "nodemon": "^1.19.4",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.2.2",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^2.3.2",
    "ts-jest": "^24.1.0",
    "typescript": "3.2.4",
    "webpack": "4.29.6",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --write"
  }
}
